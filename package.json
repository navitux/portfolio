{
  "name": "navitux.github.io",
  "version": "1.0.0",
  "description": "Ivan's personal web site and portfolio",
  "scripts": {
    "postinstall": "node ./node_modules/frontend-dependencies/index.js",
    "serve": "rm -rf dist/; npx @11ty/eleventy --serve",
    "test": "npx @11ty/eleventy --dryrun",
    "build": "rm -rf dist/; npx @11ty/eleventy;",
    "github": "rm -rf dist/; npx @11ty/eleventy; cp -r dist/about .; cp -r dist/blog . ; cp -r dist/img . ; cp -r dist/projects .;  cp -r dist/static . ; cp dist/index.html ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navitux/navitux.github.io.git"
  },
  "keywords": [
    "blog",
    "personal",
    "website",
    "static",
    "portfolio",
    "Ivan"
  ],
  "author": "Ivan Robles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navitux/navitux.github.io/issues"
  },
  "homepage": "https://github.com/navitux/navitux.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@sherby/eleventy-plugin-files-minifier": "^1.1.1"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "animate.css": "^4.1.1",
    "dayjs": "^1.11.7",
    "frontend-dependencies": "^2.0.0",
    "tachyons": "^4.12.0"
  },
  "frontendDependencies": {
    "target": "src/static/",
    "packages": {
      "tachyons": {
        "version": "^4.12.0",
        "src": "css/tachyons.min.css"
      },
      "animate.css": {
        "version": "^4.1.1",
        "src": "animate.min.css"
      }
    }
  }
}
